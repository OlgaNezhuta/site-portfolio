body
    include ../head
    .container-blog-page
        include ../common/_header
        include ../common/_mixins
        .blog-svg
            svg.svg-blog_header-dims
                title Blog-svg
                use(xlink:href="assets/img/svg-sprite.svg#blog_header")
        .blog-name-container
            .phrase-line.phrase-line1
            .blog-name-title Блог
            .phrase-line.phrase-line2
        .blog-header-phrase Статьи, которые я написала
        //include ../common/_header
    
        
        .fixed
